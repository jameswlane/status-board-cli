{
  "name": "status-board-cli",
  "version": "0.0.0-development",
  "description": "CLI utility for status board",
  "main": "./lib/caporal.js",
  "bin": {
    "status-board-cli": "./bin/cli.js"
  },
  "repository": "git@github.com:jameswlane/status-board-cli.git",
  "author": "James W. Lane <james.w.lane@mac.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "tslint": "tslint --config tslint.json --project tsconfig.json \"src/**/*.ts\"",
    "tslint:fix": "tslint --config tslint.json --project tsconfig.json --fix \"src/**/*.ts\"",
    "verify": "yarn verify:npmvet && yarn verify:david && yarn verify:nsp && yarn verify:tscheck && yarn tslint && yarn test",
    "verify:npmvet": "npmvet --renderer ci --strict",
    "verify:david": "david",
    "verify:nsp": "nsp check --output summary",
    "verify:check": "depcheck",
    "verify:tscheck": "types-checker",
    "setup": "npm i -g npmvet david nsp depcheck types-checker commitizen semantic-release-cli",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "react"
  ],
  "bugs": {
    "url": "https://github.com/jameswlane/status-board-cli/issues"
  },
  "homepage": "https://github.com/jameswlane/status-board-cli#readme",
  "dependencies": {
    "async": "2.6.0",
    "caporal": "0.9.0",
    "chalk": "2.3.0",
    "debug": "3.1.0",
    "ejs": "2.5.7",
    "jsonfile": "4.0.0",
    "mkdirp": "0.5.1",
    "sanitize-filename": "1.6.1",
    "semver": "5.4.1",
    "status-board": "1.1.12",
    "tracer": "0.8.11",
    "underscore": "1.8.3",
    "winston": "2.4.0",
    "xtend": "4.0.1"
  },
  "devDependencies": {
    "@types/async": "2.0.45",
    "@types/chalk": "2.2.0",
    "@types/debug": "0.0.30",
    "@types/ejs": "2.5.0",
    "@types/jsonfile": "4.0.1",
    "@types/mkdirp": "0.5.2",
    "@types/node": "8.5.1",
    "@types/sanitize-filename": "1.1.28",
    "@types/semver": "5.4.0",
    "@types/underscore": "1.8.5",
    "@types/winston": "2.3.7",
    "@types/xtend": "4.0.2",
    "coveralls": "3.0.0",
    "cz-conventional-changelog": "2.1.0",
    "jest": "21.2.1",
    "semantic-release": "8.2.0",
    "ts-jest": "21.2.4",
    "tslint": "5.8.0",
    "tslint-config-airbnb": "5.4.2",
    "typescript": "2.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
